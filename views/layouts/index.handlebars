

<div class="alert {{class}}" role="alert">
  {{message}}
</div>
<div class="container">
<div class="row">
</div>
<div class="row mt-3">
<div class="col-md-12">
  <form action="/" method="POST" class="mb-4" onsubmit="return validateForm()" name="myForm">
      <div class="form-group">
        <label for="url">Product Url</label>
        <input type="text" name="url" value="https://fishpondusa.com/product" class="form-control">
      </div>
      <input type="submit" value="Get Products" class="btn btn-primary btn-block">
    </form>

     <form action="/addproducts" method="POST" class="mb-4">
      <div class="form-group">
      <input type="submit" value="Add Products" class="btn btn-danger btn-block">
    </form>

    <form action="/import" method="POST" class="mt-4">
      <div class="form-group">
      <input type="submit" value="Import Products" class="btn btn-success btn-block">
    </form>

  </div>

</div>

</div>